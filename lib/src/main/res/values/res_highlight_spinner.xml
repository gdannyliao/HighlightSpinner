<?xml version="1.0" encoding="utf-8"?>


<resources xmlns:tools="http://schemas.android.com/tools">
	<declare-styleable name="HighlightSpinner"
		tools:ignore="ResourceName">
		<attr name="highlight_spinner_arrow_tint"
			format="color"/>
		<attr name="highlight_spinner_hide_arrow"
			format="boolean"/>
		<attr name="highlight_spinner_background_color"
			format="color"/>
		<attr name="highlight_spinner_text_color"
			format="color"/>
		<attr name="highlight_spinner_dropdown_max_height"
			format="dimension"/>
		<attr name="highlight_spinner_dropdown_height"
			format="dimension">
			<enum name="fill_parent"
				value="-1"/>
			<enum name="match_parent"
				value="-1"/>
			<enum name="wrap_content"
				value="-2"/>
		</attr>
		<attr name="highlight_spinner_dropdown_width"
			format="dimension">
			<enum name="fill_parent"
				value="-1"/>
			<enum name="match_parent"
				value="-1"/>
			<enum name="wrap_content"
				value="-2"/>
			<enum name="match_spinner"
				value="-3"/>
		</attr>
	</declare-styleable>

	<color name="gray_f9">#f9f9f9</color>
	<color name="highlightSpinnerGold">#D3B263</color>

	<dimen name="highlight_spinner__padding_left">24dp</dimen>
	<dimen name="highlight_spinner__padding_top">12dp</dimen>
	<dimen name="highlight_spinner__item_height">48dp</dimen>
	<dimen name="highlight_spinner__popup_padding_left">24dp</dimen>
	<dimen name="highlight_spinner__popup_padding_top">12dp</dimen>
</resources>